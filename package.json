{
  "name": "@zeix/ueli",
  "version": "1.0.2",
  "keywords": [
    "css",
    "sass",
    "scss",
    "framework"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeixcom/ueli/tree/feature/update-dep-2023"
  },
  "author": "Sergio Lavanga <sergio.lavanga@zeix.com>",
  "scripts": {
    "start": "npm run serve",
    "build": "webpack --config webpack/webpack.dev.config.js --mode=development",
    "watch": "webpack --config webpack/webpack.dev.config.js --mode=development --watch",
    "serve": "webpack serve --config webpack/webpack.dev.config.js --mode=development",
    "prod": "webpack --config webpack/webpack.prod.config.js --mode=production",
    "lint": "stylelint --fix scss/**/*",
    "prettier": "prettier --write scss/**/*",
    "sassdoc": "sassdoc scss/**/* -d docs/sassdoc",
    "test": "jest",
    "release": "npm i && npm run build && npm run sassdoc"
  },
  "devDependencies": {
    "@zeix/stylelint-config": "github:zeixcom/stylelint-config#feature/update-dep-2023",
    "autoprefixer": "^10.4.16",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "sass": "^1.69.4",
    "sass-loader": "^13.3.2",
    "sass-true": "^7.0.0",
    "sassdoc": "^2.7.4",
    "stylelint": "^15.11.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "sass-mq": "^6.0.0"
  }
}
