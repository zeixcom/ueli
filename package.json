{
  "name": "@zeix/ueli",
  "version": "1.0.0-alpha.6",
  "keywords": [
    "css",
    "sass",
    "scss",
    "framework"
  ],
  "license": "MIT",
  "author": "Sergio Lavanga <sergio.lavanga@zeix.com>",
  "scripts": {
    "start": "npm run serve",
    "build": "webpack --config webpack/webpack.dev.config.js --mode=development",
    "watch": "webpack --config webpack/webpack.dev.config.js --mode=development --watch",
    "serve": "webpack serve --config webpack/webpack.dev.config.js --mode=development",
    "prod": "webpack --config webpack/webpack.prod.config.js --mode=production",
    "lint": "stylelint --fix scss/**/*",
    "prettier": "prettier --write scss/**/*",
    "sassdoc": "sassdoc scss/**/* -d docs",
    "test": "jest"
  },
  "devDependencies": {
    "@zeix/stylelint-config": "^1.0.0-beta.6",
    "autoprefixer": "^10.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.2.5",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "sass-true": "^6.0.1",
    "sassdoc": "^2.7.3",
    "stylelint": "^14.5.3",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "sass-mq": "^6.0.0"
  }
}
